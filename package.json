{
  "homepage": "https://jamsanz.github.io/mediaplus-client",
  "name": "mediaplus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "test": "jest -c test/jest.config.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended"
    ],
    "rules": {
      "no-empty-function": "error",
      "no-empty-pattern": "error",
      "no-multi-spaces": "error",
      "no-multi-str": "error",
      "require-await": "error",
      "array-bracket-spacing": [
        "error",
        "never"
      ],
      "block-spacing": "error",
      "brace-style": "error",
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "comma-style": [
        "error",
        "last"
      ],
      "func-call-spacing": [
        "error",
        "never"
      ],
      "indent": [
        "error",
        2
      ],
      "key-spacing": [
        "error",
        {
          "beforeColon": false
        }
      ],
      "keyword-spacing": [
        "error",
        {
          "before": true
        }
      ],
      "max-depth": [
        "error",
        4
      ],
      "max-len": [
        "error",
        {
          "code": 100,
          "tabWidth": 2
        }
      ],
      "semi": [
        "error",
        "always"
      ],
      "semi-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "semi-style": [
        "error",
        "last"
      ],
      "no-undef": "off"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@heroicons/react": "^1.0.4",
    "@reduxjs/toolkit": "^1.6.0",
    "@types/js-cookie": "^3.0.0",
    "@types/toastr": "^2.1.39",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "js-cookie": "^3.0.1",
    "next": "10.2.0",
    "next-compose-plugins": "^2.2.1",
    "react": "17.0.2",
    "react-bootstrap": "^1.4.3",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.4",
    "sweetalert2": "^11.1.7",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^26.6.3",
    "nock": "^13.0.6",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
